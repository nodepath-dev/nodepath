# Database connection URL (required)
# Example: postgresql://username:password@localhost:5432/database_name
DATABASE_URL=

# SMTP configuration for email (optional)
# Host for SMTP server
SMTP_HOST=

# Port for SMTP server (usually 587 for TLS or 465 for SSL)
SMTP_PORT=

# Username for SMTP authentication
SMTP_USER=

# Password for SMTP authentication
SMTP_PASS=

# Frontend URL (optional)
# The base URL of the frontend application
FE_URL=

# From email address (optional)
# Email address used as the sender for outgoing emails
FROM_EMAIL=